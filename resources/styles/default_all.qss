 QMenu {
     /*background-color: white;*/
     margin: 0px; /* some spacing around the menu */
	 border: 1px solid #aaa;
 }

 QMenu::item {
	 margin-left: 2px;
	 margin-right: 2px;
     padding: 2px 25px 2px 20px;
     border: 1px solid transparent; /* reserve space for selection border */
 }

 QMenu::item:selected {
     background: #ddd;
	 color: black;
 }

 QMenu::icon:checked { /* appearance of a 'checked' icon */
     background: gray;
     border: 1px inset gray;
     position: absolute;
     top: 1px;
     right: 1px;
     bottom: 1px;
     left: 1px;
 }

 QMenu::separator {
     height: 1px;
     background: #ccc;
 }

 QMenu::indicator {
     width: 13px;
     height: 13px;
 }

QToolButton {
