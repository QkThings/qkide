QMenuBar {
	background: #ddd;
}

QMenuBar::item {
    spacing: 3px;
    padding: 1px 4px;
    background: #ddd;
    /*border-radius: 2px;*/
}

QMenuBar::item:selected {
    /*background: #4a79ad;*/
	background: #ff7f2a;
