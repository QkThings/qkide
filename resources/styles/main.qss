QMenuBar
{
    background: #444;
    color: white;
    padding: 0px;
}

QMenuBar::item
{
    spacing: 3px;
    padding: 3px 5px;
    background: #444;
}

QMenuBar::item:selected
{
    background: #1283de;
    color: white;
}

QMenuBar::item:pressed
{
    background: #1283de;
}

QToolTip
{
    color: #444;
    padding: 1px;
    border-radius: 2px;
    background-repeat:no-repeat;
    background-position:center;
    background-color: #FEFFE8;
}
